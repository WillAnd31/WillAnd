{
  "name": "willand",
  "version": "1.0.0",
  "description": "Personal Website",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WillAnd31/willand.git"
  },
  "author": "Will Anderson",
  "license": "MIT",
  "homepage": "https://willand.co",
  "scripts": {
    "postinstall": "typings install",
    "clean": "rm -rf node_modules typings dist",
    "build": "npm run build:dev",
    "build:clean": "rm -rf ./dist && rm -rf ./dist-tmp",
    "build:dev": "npm run server:dev",
    "build:prod": "NODE_ENV=prod npm run build:general",
    "build:staging": "NODE_ENV=staging npm run build:general",
    "build:general": "webpack --config webpack.config.js --display-error-details --progress && npm run build:post",
    "build:post": "rm -rf dist && cp -r ./dist-tmp ./dist && rm -rf ./dist-tmp",
    "build:watch": "npm run build:clean && NODE_ENV=dev webpack --config webpack.config.js --watch --display-error-details --progress",
    "server:watch": "NODE_ENV=dev nodemon --debug ./server.js --watch server.js",
    "start": "npm run build:dev",
    "server:dev": "npm run server:watch & npm run build:watch",
    "start:staging": "npm run build:staging && pm2 startOrRestart ecosystem.json",
    "start:prod": "npm run build:prod && pm2 startOrRestart ecosystem.json --env prod"
  },
  "dependencies": {
    "@angular/common": "^2.0.0-rc.5",
    "@angular/compiler": "^2.0.0-rc.5",
    "@angular/core": "^2.0.0-rc.5",
    "@angular/http": "^2.0.0-rc.5",
    "@angular/platform-browser": "^2.0.0-rc.5",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.5",
    "@angular/router": "^3.0.0-rc.1",
    "angular2-template-loader": "^0.5.0",
    "angulartics2": "^1.1.2",
    "autoprefixer": "^6.3.7",
    "core-js": "^2.4.1",
    "css-loader": "^0.23.1",
    "css-to-string-loader": "^0.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "fs": "0.0.2",
    "glob": "^7.0.4",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.21.0",
    "imports-loader": "^0.6.5",
    "jasmine-core": "^2.4.1",
    "json-loader": "^0.5.4",
    "jsonwebtoken": "^7.0.1",
    "jwplayer": "^1.0.1",
    "lodash": "^4.14.1",
    "ng2-translate": "^2.2.2",
    "node-sass": "^3.7.0",
    "path": "^0.12.7",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "^0.1.3",
    "request": "^2.72.0",
    "resolve-url-loader": "^1.4.3",
    "restify": "^4.1.1",
    "rxjs": "^5.0.0-beta.10",
    "sass-loader": "^3.2.0",
    "script-loader": "^0.7.0",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.4",
    "ts-loader": "^0.8.2",
    "typescript": "^1.8.10",
    "util": "^0.10.3",
    "webpack": "^1.13.1",
    "webpack-merge": "^0.14.0",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "webpack-livereload-plugin": "^0.8.1"
  }
}
