{
  "name": "willand",
  "version": "1.0.0",
  "description": "Personal Website",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WillAnd31/willand.git"
  },
  "author": "Will Anderson",
  "license": "MIT",
  "homepage": "https://willand.co",
  "scripts": {
    "clean": "rm -rf node_modules typings dist",
    "build": "npm run build:prod",
    "start": "npm run server & npm run build:dev",
    "copy:images": "cp -R client/images/* dist/assets",
    "move:dist-tmp": "mv dist-tmp dist",
    "start:prod": "npm run build:prod && pm2 startOrRestart ecosystem.json",
    "server": "node server.js",
    "build:clean": "rm -rf dist",
    "build:dev": "npm run build:clean && NODE_ENV=dev npm run build:general",
    "build:staging": "npm run build:clean && NODE_ENV=staging npm run build:general",
    "build:prod": "NODE_ENV=prod npm run build:general && npm run build:clean && npm run move:dist-tmp && npm run copy:images",
    "build:general": "webpack --config webpack.config.js"
  },
  "dependencies": {
    "@angular/common": "^2.4.9",
    "@angular/compiler": "^2.4.9",
    "@angular/compiler-cli": "^2.4.9",
    "@angular/core": "^2.4.9",
    "@angular/http": "^2.4.9",
    "@angular/platform-browser": "^2.4.9",
    "@angular/platform-browser-dynamic": "^2.4.9",
    "@angular/router": "^3.4.9",
    "@ngtools/webpack": "^1.2.13",
    "@types/core-js": "^0.9.35",
    "@types/lodash": "^4.14.55",
    "@types/node": "^7.0.5",
    "angular2-template-loader": "^0.6.2",
    "autoprefixer": "^6.7.7",
    "awesome-typescript-loader": "^3.1.2",
    "core-js": "^2.4.1",
    "css-loader": "^0.27.3",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.17.4",
    "ng2-translate": "^5.0.0",
    "node-sass": "^4.5.0",
    "path": "^0.12.7",
    "postcss-loader": "^1.3.3",
    "precss": "^1.4.0",
    "resolve-url-loader": "^2.0.2",
    "restify": "^4.3.0",
    "rxjs": "^5.2.0",
    "sass-loader": "^6.0.3",
    "typescript": "^2.2.1",
    "webpack": "^2.2.1",
    "webpack-merge": "^4.0.0",
    "zone.js": "^0.8.2"
  },
